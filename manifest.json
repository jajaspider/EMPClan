{
  "manifest_version": 2,

  "name": "EMP Clan One Click Checker",
  "description": "EMP 클랜 공지노트 댓글 확인 프로그램",
  "version": "0.1",
  "background": {
      "scripts": [
          "background.js"
      ],
      "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "sample-imagemoa"
  },
  "permissions": [
    "https://*/*",
    "http://*/*"
  ]
}
